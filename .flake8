[flake8]
ignore = E127,E201,E202,E203,E231,E252,E266,E402,E999,F841,W503,W504,W605,F403,F401, E731
select = C,E,F,W,B,B950
extend-ignore = E131,E125,E126,E203,E251,E501
#extend-ignore = F401 # ignore unused
max-line-length = 88
exclude = .git,docs,scripts,build,dist,versioneer.py,conftest.py

per-file-ignores =
     */*__init__.py:F401
     hdf5_utils.py:E722, 
